{
  "name": "willdurand/geocoder-bundle",
  "description": "Integration of Geocoder into Symfony",
  "keywords": ["geocoding", "geocoder"],
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "William Durand",
      "email": "will+git@drnd.me"
    }
  ],
  "require": {
    "php": "^7.0",
    "willdurand/geocoder": "^4.0",
    "symfony/framework-bundle": "^3.3 || ^4.0",
    "symfony/options-resolver": "^3.3 || ^4.0",
    "symfony/console": "^3.3 || ^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2",
    "symfony/yaml": "^3.3 || ^4.0",
    "geoip/geoip": "~1.17",
    "doctrine/cache": "~1.3",
    "doctrine/orm": "~2.3",
    "php-http/message": "^1.5",
    "geocoder-php/google-maps-provider": "^4.0",
    "geocoder-php/cache-provider": "^4.0",
    "php-http/curl-client": "^1.7",
    "nyholm/psr7": "^0.2.2",
    "nyholm/symfony-bundle-test": "^1.1"
  },
  "conflict":  {
    "willdurand/geocoder": "3.2"
  },
  "autoload": {
    "psr-4": {
      "Bazinga\\GeocoderBundle\\": ""
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --testsuite main",
    "test-doctrine": "vendor/bin/phpunit --testsuite doctrine",
    "test-ci": "vendor/bin/phpunit --testsuite main --coverage-text --coverage-clover=build/coverage.xml"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "5.0-dev"
    }
  }
}
