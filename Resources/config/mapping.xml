<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="bazinga_geocoder.mapping.driver.class">Bazinga\Bundle\GeocoderBundle\Mapping\Driver\AnnotationDriver</parameter>
    </parameters>

    <services>
        <service id="bazinga_geocoder.mapping.driver" class="%bazinga_geocoder.mapping.driver.class%">
            <argument type="service" id="annotations.reader" />
        </service>
    </services>
</container>